{
  "name": "hypha-core",
  "version": "0.20.84",
  "description": "Hypha Core",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=development",
    "build:deno": "node scripts/build-deno.js",
    "build:all": "npm run build && npm run build:deno",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --require test/setup.js test/unit/**/*.test.js --timeout 10000",
    "test:integration": "playwright test",
    "test:watch": "mocha --require test/setup.js test/unit/**/*.test.js --watch",
    "test:coverage": "c8 npm run test:unit",
    "serve:tests": "node scripts/serve-tests.js",
    "publish-npm": "npm run build:all && npm publish --access public"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "deno": "./deno.js",
      "import": "./index.mjs",
      "require": "./index.js",
      "browser": "./index.mjs",
      "node": "./index.js",
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./deno-websocket-server": {
      "deno": "./dist/deno/deno-websocket-server.js",
      "import": "./dist/deno/deno-websocket-server.js",
      "types": "./deno-websocket-server.d.ts"
    }
  },
  "author": "Wei Ouyang",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@playwright/test": "^1.53.1",
    "babel-loader": "^9.1.2",
    "c8": "^8.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "esbuild-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jsdom": "^22.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "playwright": "^1.53.1",
    "sinon": "^15.2.0",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "tailwindcss": "^3.4.4",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@babel/standalone": "^7.24.7",
    "@monaco-editor/react": "^4.6.0",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "chai": "^4.3.4",
    "hypha-rpc": "^0.20.61",
    "imjoy-rpc": "^0.5.59",
    "mocha": "^8.4.0",
    "mock-socket": "^9.3.1",
    "path-browserify": "^1.0.1",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-codemirror2": "^8.0.0",
    "react-dom": "^18.2.0",
    "source-map": "^0.7.3",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  }
}
